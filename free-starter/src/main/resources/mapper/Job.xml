<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.uiautofree.runner.dao.JobMapper">

    <resultMap id="BaseResultMap" type="com.uiautofree.runner.domain.Job" >
        <result column="id" property="id" />
        <result column="gmt_create" property="gmtCreate" />
        <result column="gmt_modified" property="gmtModified" />
        <result column="batch_id" property="batchId" />
        <result column="case_id" property="caseId" />
        <result column="case_record_id" property="caseRecordId" />
        <result column="resource_id" property="resourceId" />
        <result column="status" property="status" />
        <result column="is_deleted" property="isDeleted" />
        <result column="extension" property="extension" />
    </resultMap>

    <sql id="Base_Column_List">
        id,
        gmt_create,
        gmt_modified,
        batch_id,
        case_id,
        case_record_id,
        resource_id,
        status,
        is_deleted,
        extension
    </sql>

    <insert id="insert" useGeneratedKeys="true" keyColumn="id" keyProperty="id" parameterType="com.uiautofree.runner.domain.Job">
        INSERT INTO job
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="null != gmtCreate and '' != gmtCreate">
                gmt_create,
            </if>
            <if test="null != gmtModified and '' != gmtModified">
                gmt_modified,
            </if>
            <if test="null != batchId and '' != batchId">
                batch_id,
            </if>
            <if test="null != caseId and '' != caseId">
                case_id,
            </if>
            <if test="null != caseRecordId and '' != caseRecordId">
                case_record_id,
            </if>
            <if test="null != resourceId and '' != resourceId">
                resource_id,
            </if>
            <if test="null != status and '' != status">
                status,
            </if>
            <if test="null != isDeleted and '' != isDeleted">
                is_deleted,
            </if>
            <if test="null != extension and '' != extension">
                extension
            </if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="null != gmtCreate and '' != gmtCreate">
                #{gmtCreate},
            </if>
            <if test="null != gmtModified and '' != gmtModified">
                #{gmtModified},
            </if>
            <if test="null != batchId and '' != batchId">
                #{batchId},
            </if>
            <if test="null != caseId and '' != caseId">
                #{caseId},
            </if>
            <if test="null != caseRecordId and '' != caseRecordId">
                #{caseRecordId},
            </if>
            <if test="null != resourceId and '' != resourceId">
                #{resourceId},
            </if>
            <if test="null != status and '' != status">
                #{status},
            </if>
            <if test="null != isDeleted and '' != isDeleted">
                #{isDeleted},
            </if>
            <if test="null != extension and '' != extension">
                #{extension}
            </if>
        </trim>
    </insert>

    <delete id="delete" >
        DELETE FROM job
        WHERE id = #{id}
    </delete>

    <update id="update" parameterType="com.uiautofree.runner.domain.Job">
        UPDATE job
        <set>
            <if test="null != gmtCreate and '' != gmtCreate">gmt_create = #{gmtCreate},</if>
            <if test="null != gmtModified and '' != gmtModified">gmt_modified = #{gmtModified},</if>
            <if test="null != batchId and '' != batchId">batch_id = #{batchId},</if>
            <if test="null != caseId and '' != caseId">case_id = #{caseId},</if>
            <if test="null != caseRecordId and '' != caseRecordId">case_record_id = #{caseRecordId},</if>
            <if test="null != resourceId and '' != resourceId">resource_id = #{resourceId},</if>
            <if test="null != status and '' != status">status = #{status},</if>
            <if test="null != isDeleted and '' != isDeleted">is_deleted = #{isDeleted},</if>
            <if test="null != extension and '' != extension">extension = #{extension}</if>
        </set>
        WHERE id = #{id}
    </update>


    <select id="load" resultMap="BaseResultMap">
        SELECT <include refid="Base_Column_List" />
        FROM job
        WHERE id = #{id}
    </select>

    <select id="pageList" resultMap="BaseResultMap">
        SELECT <include refid="Base_Column_List" />
        FROM job
        LIMIT #{offset}, #{pageSize}
    </select>

    <select id="pageListCount" resultType="java.lang.Integer">
        SELECT count(1)
        FROM job
    </select>

</mapper>